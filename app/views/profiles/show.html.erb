<% if @talent.present? %>
  <% content_for :meta_title, "#{@talent["name"]} | #{DEFAULT_META["meta_product_name"]}" %>
  <% if @talent["headline"].present? %>
    <% content_for :meta_description, @talent["headline"] %>
  <% end %>
  <% if @talent["profile_picture_url"].present? %>
    <% content_for :meta_image, @talent["profile_picture_url"] %>
    <% content_for :meta_card_type, "summary" %>
  <% end %>
<% end %>

<% if current_acting_user.nil? %>
  <%= react_component("LoggedOutTopBar", props: {}, prerender: false) %>
<% end %>

<%= react_component("ProfilePage", props: {}, prerender: false) %>
